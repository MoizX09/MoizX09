    "def build_person(first, last, age=None):\n",
    "    person = {'first': first, 'last': last}\n",
    "    if age:\n",
    "        person['age'] = age\n",
    "\n",
    "    return person\n",
    "\n",
    "\n",
    "musician = build_person('Borde', 'Jenifus', 27)\n",
    "print(musician)\n",
    "musician = build_person('Jack', 'Kiyler')\n",
    "print(musician)"
   ]
  }
