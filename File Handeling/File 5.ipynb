   "source": [
    "# Creating txt file and adding data from other txt file.\n",
    "\n",
    "\n",
    "file = \"file.txt\"\n",
    "\n",
    "with open(file, 'r') as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "\n",
    "with open(\"new_file1.txt\", 'w+') as f:\n",
    "    for line in lines:\n",
    "        f.write(line)"
   ]
  }
