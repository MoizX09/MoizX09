   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Body.\n",
    "\n",
    "n = np.linspace(0, 4*np.pi, 100)\n",
    "sin = np.sin(n)\n",
    "plt.plot(n, sin)\n",
    "plt.xlabel(\"Time\")\n",
    "plt.ylabel(\"Amplitude\")\n",
    "plt.title(\"Sin Wave\")\n",
    "plt.xlim([0, 4*np.pi])\n",
    "plt.ylim(-1.5, 1.5)\n",
    "plt.show()"
   ]
  }
